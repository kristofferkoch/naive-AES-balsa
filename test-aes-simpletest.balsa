import [balsa.sim.portio]
import [aes]

procedure balsa
is
  channel i : 32 bits
  channel o : 32 bits
begin
  aes (i, o) ||
  BalsaInputPortFromFile (32 bits, "i", <- FileOpen ("aes.dat", read), i) ||
  BalsaOutputPortToLogWithFormat (32 bits, "o", 16, 0, 1, o)
end
