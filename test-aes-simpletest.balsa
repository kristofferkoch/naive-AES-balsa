import [teak.sim.portio]
import [aes]

procedure balsa
is
  channel inp : blocks
  channel o : blocks
  channel file : File
begin
  aes (inp, o) ||
  file <- FileOpen ("aes.dat", read) ||
  BalsaInputPortFromFile (blocks, "inp", file (-- <- FileOpen ("aes.dat", read) --), inp) ||
  BalsaOutputPortToLog (blocks, "o", o)
end
