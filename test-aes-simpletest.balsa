import [balsa.sim.portio]
import [aes]

procedure balsa
is
  channel inp : blocks
  channel o : blocks
begin
  aes (inp, o) ||
  BalsaInputPortFromFile (blocks, "inp", <- FileOpen ("aes.dat", read), inp) ||
  BalsaOutputPortToLog (blocks, "o", o)
end
